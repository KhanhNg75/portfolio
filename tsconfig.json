{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@definitions/*": [
        "src/definitions/*"
      ],
      "@definitions": [
        "src/definitions"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@interfaces": [
        "src/interfaces"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@components": [
        "src/components"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@styles": [
        "src/styles"
      ],
      "@public/*": [
        "public/*"
      ],
      "@public": [
        "public"
      ],
      "@components/icons/*": [
        "src/components/icons/*"
      ],
      "@components/icons": [
        "src/components/icons"
      ],
      "@i18n": [
        "./i18n"
      ],
      "@assets/*": ["src/assets/*"],
      "@assets": ["src/assets"]
    },
    "incremental": true
  },
  "include": ["next-env.d.ts", "declaration.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": [
    "node_modules"
  ]
}
